{
  "name": "kobesoft/green-auth-core",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Green\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Green\\Auth\\Tests\\": "tests/"
    }
  },
  "require": {
    "filament/filament": "^3.3",
    "kalnoy/nestedset": "^6.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "orchestra/testbench": "^8.0",
    "laravel/framework": "*"
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Green\\Auth\\GreenAuthServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
